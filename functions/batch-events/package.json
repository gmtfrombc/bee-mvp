{
    "name": "bee-batch-events",
    "version": "1.0.0",
    "description": "BEE Core Engagement Module - Batch Events Import Cloud Function",
    "main": "main.js",
    "scripts": {
        "start": "node main.js",
        "dev": "nodemon main.js",
        "test": "jest",
        "deploy": "gcloud functions deploy batchEvents --runtime nodejs18 --trigger-http --allow-unauthenticated",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.38.0",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "moment-timezone": "^0.5.43",
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "jest": "^29.7.0",
        "eslint": "^8.52.0",
        "prettier": "^3.0.3",
        "@types/node": "^20.8.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "bee",
        "engagement",
        "events",
        "batch",
        "import",
        "supabase",
        "cloud-function"
    ],
    "author": "BEE Development Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/your-org/bee-mvp.git",
        "directory": "functions/batch-events"
    },
    "jest": {
        "testEnvironment": "node",
        "collectCoverage": true,
        "coverageDirectory": "coverage",
        "testMatch": [
            "**/__tests__/**/*.js",
            "**/?(*.)+(spec|test).js"
        ]
    },
    "eslintConfig": {
        "env": {
            "node": true,
            "es2021": true,
            "jest": true
        },
        "extends": [
            "eslint:recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "rules": {
            "no-console": "off",
            "no-unused-vars": [
                "error",
                {
                    "argsIgnorePattern": "^_"
                }
            ]
        }
    }
}